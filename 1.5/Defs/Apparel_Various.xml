<Defs>

	<VFEPirates.WarcasketDef ParentName="VFEP_WarcasketHelmetBase">
    <defName>WarcasketHelmet_Faz</defName>
    <label>freddy fazcasket head</label>
    <shortDescription>a warcasket helmet designed in the style of a bear.</shortDescription>
    <description>Nobody stuffs bodies into suits like afton robotics! Buy this brand new Freddy Fazcasket, scrapped from our facility. The teeth look sharp enough to bite a head off.</description>	  
    <graphicData>
      <texPath>Things/Pawn/Warcasketlike/Faz/WarcasketHelmet</texPath>
    </graphicData>
    <uiIconScale>1.25</uiIconScale>
    <statBases>
      <Mass>5</Mass>
      <ArmorRating_Sharp>1.06</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.55</ArmorRating_Blunt>
      <ArmorRating_Heat>0.64</ArmorRating_Heat>
      <Insulation_Cold>4</Insulation_Cold>
      <Insulation_Heat>2</Insulation_Heat>
      <EquipDelay>1</EquipDelay>
    </statBases>
    <equippedStatOffsets>
	  <MoveSpeed>-0.1</MoveSpeed>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.1</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <costList>
      <ComponentIndustrial>1</ComponentIndustrial>
      <Steel>40</Steel>
    </costList>
    <researchPrerequisites>
      <li>VFEP_Warcaskets</li>
    </researchPrerequisites>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Warcasketlike/Faz/WarcasketHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>Warcasket</li>
		<li>WarcasketAll</li>
      </tags>
    </apparel>
  </VFEPirates.WarcasketDef>	
  
