	<VFEPirates.WarcasketDef ParentName="VFEP_WarcasketArmorBase">
		<defName>Warcasket_joris</defName>
		<label>jorcasket body</label>
		<shortDescription>a warcasket designed to look barebones, yet still animalistic.</shortDescription>
		<description>The nullcasket body is used often, due to it's generic and inoffensive look. Originally a part of the joris set.</description>
		<graphicData>
			<texPath>Things/Pawn/Warcasketlike/Polar/Warcasket</texPath>
		</graphicData>
		<statBases>
			<Mass>45</Mass>
			<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.6</ArmorRating_Blunt>
			<ArmorRating_Heat>0.8</ArmorRating_Heat>
			<Insulation_Cold>20</Insulation_Cold>
			<Insulation_Heat>10</Insulation_Heat>
			<EquipDelay>1</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.2</MoveSpeed>
			<SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.3</SlaveSuppressionOffset>
		</equippedStatOffsets>
		<costList>
			<ComponentIndustrial>4</ComponentIndustrial>
			<Steel>90</Steel>
			<Uranium>10</Uranium>
		</costList>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Legs</li>
				<li>Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Warcasketlike/Polar/Warcasket</wornGraphicPath>
			<layers>
			    <li>OnSkin</li>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Warcasket</li>
				<li>WarcasketAll</li>
			</tags>
		</apparel>
		<researchPrerequisites>
			<li>VFEP_Warcaskets</li>
		</researchPrerequisites>
		<modExtensions>
			<li Class="VFECore.ApparelExtension">
				<traitsOnEquip>
					<li>VFEP_WarcasketTrait</li>
				</traitsOnEquip>
				<traitsOnUnequip>
					<li>VFEP_Shellcasket</li>
				</traitsOnUnequip>
				<pawnCapacityMinLevels>
					<li>
						<capacity>Moving</capacity>
						<minLevel>0.7</minLevel>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<minLevel>0.7</minLevel>
					</li>
				</pawnCapacityMinLevels>
			</li>
		</modExtensions>
	</VFEPirates.WarcasketDef>
	
